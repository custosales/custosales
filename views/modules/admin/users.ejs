<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
     <%- include('head') %>
  </head>
  <body>
    <nav>
      <%- include('menu') %>
    </nav>
    <main>
    <h1><%= title %></h1>
    <p>Velkommen til <%= title %></p>

    <table id="tabell">
      <thead>
          <tr>
              <th>Navn</th>
              <th>Brukernavn</th>
              <th>Epost</th>
              <th>Passord</th>
              <th>Dato</th>
          </tr>

      </thead>
      <tbody>
          <%  for(var i = 0; i < docs.length; i++) {  %>

              <tr>
                  <td>
                      <%= docs[i].navn %>
                  </td>
                  <td>
                      <%= docs[i].brukernavn %>
                  </td>

                  <td>
                      <%= docs[i].epost %>
                  </td>
                  <td>
                      <%= docs[i].passord %>
                  </td>
                  <td>
                      <%= docs[i].dato %>
                  </td>

              </tr>

              <% }  %>
      </tbody>

  </table>


  </main>
  </body>
</html>
